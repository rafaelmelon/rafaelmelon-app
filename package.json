{
  "name": "rafaelmelon",
  "version": "0.0.1",
  "description": "Rafael Melón - Front-end Developer SPA Portfolio",
  "private": true,
  "scripts": {
    "launch": "node server/express.js",
    "build": "yarn run clean && webpack -p --config configs/webpack/prod.js --progress",
    "clean": "rm -f -r -d dist",
    "lint": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}' --fix --format stylish",
    "start": "yarn run build && yarn run launch",
    "dev": "webpack-dev-server -d --config configs/webpack/dev.js --progress",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelmelon/rafaelmelon.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "typescript",
    "styled-components",
    "redux",
    "heroku",
    "prettier"
  ],
  "author": "Rafael Melón <info@rafaelmelon.es> (http://rafaelmelon.es/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafaelmelon/rafaelmelon/issues"
  },
  "homepage": "https://github.com/rafaelmelon/rafaelmelon#readme",
