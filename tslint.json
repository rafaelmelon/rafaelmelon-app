{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "variable-name": false,
    "jsx-wrap-multiline": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "import-name": false,
    "no-var-requires": false,
    "prettier": true
  }
}